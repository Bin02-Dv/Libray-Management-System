{
  "version": 2,
  "buildCommand": "python manage.py collectstatic --noinput",
  "builds": [
    {
      "src": "LYMS/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.13"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "LYMS/wsgi.py"
    }
  ]
}
