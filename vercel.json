{
  "version": 2,
  "builds": [
      {
      "src": "LYMS/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.13"
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "LYMS/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles" 
}